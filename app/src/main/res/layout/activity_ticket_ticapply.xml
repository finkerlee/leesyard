<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lijiadayuan.lishijituan.TicketTicapplyActivity">
    <include
        layout="@layout/common_title"
        android:id="@+id/include18" />
    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/include18"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"></WebView>

    <Button
        android:id="@+id/i_want_receive"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/rounded_button_color"
        android:text="我要领取"
        android:textColor="@color/color_white"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
